{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "indaba-backend",
            "image": "amidatech/greyscale-backend:dev",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 3005,
                    "containerPort": 3005
                }
            ],
            "environment": [
              {
                  "name": "INDABA_PG_DB",
                  "value": "indaba"
              }, {
                  "name": "AUTH_SALT",
                  "value": "nMsDo)_1fh"
              }
            ],
            "links": [
                "memcached"
            ]
        },
        {
            "name": "memcached",
            "image": "memcached:latest",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 11211,
                    "containerPort": 11211
                }
            ]
        }
    ]
}
